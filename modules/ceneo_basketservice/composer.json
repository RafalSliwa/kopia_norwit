{
  "name": "ceneo/ceneobs",
  "description": "Ceneo Basket Service",
  "autoload": {
    "psr-4": {
      "CeneoBs\\": "src/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "prepend-autoloader": false,
    "optimize-autoloader": true
  },
  "require": {
    "tcdent/php-restclient": "*",
    "ext-json": "*",
    "ext-simplexml": "*",
    "ext-libxml": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.18.0",
    "prestashop/php-dev-tools": "^3.16.1"
  },
  "authors": [
    {
      "name": "Ceneo"
    }
  ],
  "scripts": {
    "build:prod": "composer install -v --no-dev"
  },
  "type": "prestashop-module",
  "author": "Ceneo"
}
